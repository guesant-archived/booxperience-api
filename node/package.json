{
  "name": "booxperience-api",
  "version": "0.0.0",
  "main": "build/app.js",
  "repository": "https://github.com/guesant/booxperience-api.git",
  "author": "Gabriel Rodrigues",
  "license": "MIT",
  "scripts": {
    "prod:start": "npm run prod:build && npm run start",
    "prod:build": "env NODE_ENV=production rollup --config rollup.config.js",
    "start": "env NODE_ENV=production node ./build/app.js",
    "dev:watch": "rollup --config rollup.config.js --watch",
    "dev:start": "node-dev --respawn build/app.js",
    "dev": "env NODE_ENV=development concurrently 'npm:dev:watch' 'npm:dev:start'"
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "env": "0.0.2",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.11",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.10.0",
    "node-dev": "^6.2.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/cors": "^2.8.8",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/morgan": "^1.9.1",
    "@types/yup": "^0.29.9",
    "rollup": "^2.32.1"
  }
}
