{
  "name": "booxperience-api",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://github.com/guesant/booxperience-api.git",
  "author": "Gabriel Rodrigues",
  "license": "MIT",
  "scripts": {
    "dev": "npm run dev:prepare && env NODE_ENV=development concurrently 'npm:dev:watch' 'npm:dev:start'",
    "start": "env NODE_ENV=production node ./build/app.js",
    "prod:start": "npm run prod:build && npm run start",
    "prod:build": "env NODE_ENV=production rollup --config rollup.config.js",
    "dev:prepare": "rm -f build/app.js; touch build/app.js",
    "dev:watch": "rollup --config rollup.config.js --watch",
    "dev:start": "node-dev --respawn build/app.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.3.2",
    "env": "0.0.2",
    "express": "^4.17.1",
    "halson": "^3.0.0",
    "helmet": "^4.2.0",
    "morgan": "^1.10.0",
    "query-string": "^6.13.7",
    "ts-toolbelt": "^8.0.7"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "concurrently": "^5.3.0",
    "node-dev": "^6.2.0",
    "rollup": "^2.33.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  }
}
